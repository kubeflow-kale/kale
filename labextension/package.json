{
  "name": "kubeflow-kale-labextension",
  "version": "0.6.0",
  "description": "Convert Notebooks to Kubeflow pipelines with Kale",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/kubeflow-kale/kale",
  "bugs": {
    "url": "https://github.com/kubeflow-kale/kale/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Stefano Fioravanzo <stefano.fioravanzo@gmail.com>",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kubeflow-kale/kale.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf kubeflow-kale-labextension/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "format": "pretty-quick --pattern 'labextension/**/*.{ts,js,tsx,jsx,scss,css,html,md,json}'",
    "format:check": "jlpm format --check",
    "format:check:staged": "jlpm format:check --staged",
    "format:staged": "jlpm format --staged",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "prepublishOnly": "npm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.3",
    "@jupyterlab/apputils": "^3.0.2",
    "@jupyterlab/coreutils": "^5.0.1",
    "@jupyterlab/notebook": "^3.0.3",
    "@lumino/widgets": "^1.16.1",
    "@material-ui/core": "^4.4.3",
    "@material-ui/icons": "^4.4.3",
    "@types/js-yaml": "^3.12.4",
    "@types/node": "^14.0.20",
    "@types/react-dom": "^16.9.0",
    "js-yaml": "^3.14.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-sanitized-html": "^2.0.0",
    "use-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0-rc.13",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "resolutions": {
    "@types/react": "^16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "kubeflow-kale-labextension/labextension"
  },
  "husky": {
    "hooks": {
      "pre-commit": "jlpm format:check:staged"
    }
  },
  "styleModule": "style/index.js"
}